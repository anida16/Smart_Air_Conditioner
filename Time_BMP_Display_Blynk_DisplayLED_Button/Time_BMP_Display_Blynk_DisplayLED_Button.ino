void button_control()
{
  buttonState = digitalRead(buttonPin);

  //Serial.println(digitalRead(button));
  if (buttonState == 1) {
    Serial.println("Turn AC ON");
    ac.on();
    ac.setFan(3);
    ac.setMode(kGreeCool);
    ac.setTemp(27);  // 16-30C
    ac.setSwingVertical(true, kGreeSwingAuto);
    ac.setXFan(false);
    ac.setLight(true);
    ac.setSleep(true);
    ac.setTurbo(false);
  }
  if (buttonState == 0) {
    Serial.println("Turn AC OFF");
    ac.off();
  }
  if (tempo1 != buttonState) {
    Serial.println("The button value changed");

#if SEND_GREE
    Serial.println("Sending IR command to A/C ...");
    ac.send();
#endif  // SEND_GREE
    printState();
  }
  tempo1 = buttonState;
}

// main loop
void loop()
{
  timeClient.update();
  unix_epoch = timeClient.getEpochTime();   // get UNIX Epoch time

  //tft.fillScreen(ST7735_BLACK);
  Blynk.run();

  Room_temperature_display();
  mode_selection();
  button_control();

  delay(1000);    // wait 200ms

  if (tft_display == 1)
  {
    tft.writeCommand(0x10); // Sleep
    delay(5); // Delay for shutdown time before another command can be sent
    Serial.println("TFT Display off");
    tft.fillScreen(ST77XX_BLACK);
  }

  if (tft_display == 0)
  {
    RTC_display();
    tft.writeCommand(0x11); // Wake display
    delay(120); // Delay for pwer supplies to stabilise
    Serial.println("TFT Display On");
  }

}

